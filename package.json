{
  "name": "git-mcp-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/src/index.js', '755')\"",
    "test": "mocha --config .mocharc.js",
    "test:watch": "mocha --watch",
    "test:coverage": "nyc mocha",
    "git-user-history": "node build/src/git/tools/git-user-history-runner.js 2025-09-30",
    "git-repository-history": "node build/src/git/tools/git-repository-history-runner.js 2025-09-01",
    "git-message": "node build/src/git/tools/git-message-logs-runner.js",
    "git-patch": "node build/src/git/tools/git-patch-logs-runner.js",
    "git-summaries": "node build/src/git/tools/git-summary-logs-runner.js",
    "git-directory": "node build/src/git/tools/git-directory-logs-runner.js",
    "git-file-content": "node build/src/git/tools/git-file-content-runner.js",
    "batch-processing-github-all-commit-diff": "node build/src/batch-processing/multiple-repository/github-commit-diff/fetch-and-store-commit-diffs-runner.js ",
    "batch-processing-github-all-commit": "node build/src/batch-processing/multiple-repository/github-commit/fetch-and-store-commits-runner.js ",
    "batch-processing-github-commit": "node build/src/batch-processing/single-repository/github-commit/fetch-and-store-commits-runner.js ",
    "batch-processing-github-content": "node build/src/batch-processing/single-repository/github-content/fetch-and-store-content-runner.js ",
    "batch-processing-github-commit-diff": "node build/src/batch-processing/single-repository/github-commit-diff/fetch-and-store-commit-diffs-runner.js ",
    "ai-summaries-github-commit-diff": "node build/src/batch-processing/ai-summaries/github-commit-diff/fetch-and-store-commits-runner.js --maxCommits 2000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.0",
    "chai": "^6.2.0",
    "mocha": "^11.7.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "zod": "^3.25.76"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.4.0",
    "@octokit/rest": "^22.0.0",
    "dotenv": "^17.2.2",
    "mongodb": "6.19",
    "nyc": "^17.1.0",
    "simple-git": "^3.28.0"
  }
}
